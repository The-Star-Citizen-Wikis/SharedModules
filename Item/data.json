{
	"%includes": "Additional item information that should be returned",
	"includes": [
		"shops.items"
	],
	"%smw_data": "Sets which data gets added to SMW",
	"smw_data": [
		{
			"SMW_Name": [
				"API_name",
				"ARG_Name"
			],
			"smw_format": "#-"
		},
		{
			"SMW_Description": [
				"API_description"
			],
			"type": "multilingual_text"
		},
		{
			"SMW_Manufacturer": [
				"API_manufacturer.code",
				"ARG_Manufacturer"
			],
			"%format": "[[%s]]",
			"smw_format": "#-"
		},
		{
			"SMW_Type": "API_type"
		},
		{
			"SMW_Size": "API_size",
			"type": "number"
		},
		{
			"SMW_Occupancy": "API_dimension.volume",
			"type": "number"
		},
		{
			"SMW_Class": "API_class"
		},
		{
			"SMW_Grade": "API_grade"
		},
		{
			"SMW_EntityLength": [
				"API_dimension.true_dimension.length",
				"API_dimension.length"
			],
			"type": "number",
			"smw_format": "#p2-cm"
		},
		{
			"SMW_EntityWidth": [
				"API_dimension.true_dimension.width",
				"API_dimension.width"
			],
			"type": "number",
			"smw_format": "#p2-cm"
		},
		{
			"SMW_EntityHeight": [
				"API_dimension.true_dimension.height",
				"API_dimension.height"
			],
			"type": "number",
			"smw_format": "#p2-cm"
		},
		{
			"SMW_Mass": "API_mass",
			"type": "number"
		},
		{
			"SMW_GalactapediaUrl": "ARG_GalactapediaUrl",
			"smw_format": "#-"
		},
		{
			"SMW_GameBuild": "API_version"
		},
		{
			"SMW_UUID": "API_uuid",
			"smw_format": "#-"
		},
		{
			"SMW_ItemPorts": "API_ports",
			"type": "subobject",
			"smw_data": [
				{
					"SMW_ItemPortName": "API_name"
				},
				{
					"SMW_ItemPortDisplayName": "API_display_name"
				},
				{
					"SMW_ItemPortMinimumSize": "API_sizes.min",
					"type": "number"
				},
				{
					"SMW_ItemPortMaximumSize": "API_sizes.max",
					"type": "number"
				},
				{
					"SMW_EquippedItemUUID": "API_equipped_item.uuid"
				},
				{
					"SMW_EquippedItemName": "API_equipped_item.name"
				}
			]
		}
	],
	"%community_sites": "List of format strings which link to community sites",
	"community_sites": [
		{
			"label": "Universal Item Finder",
			"format": "https://finder.cstone.space/search/%s",
			"data": "SMW_UUID"
		},
		{
			"label": "Wiki API",
			"format": "https://api.star-citizen.wiki/api/v2/items/%s",
			"data": "SMW_UUID"
		}
	],
	"%official_sites": "List of attributes that link to official sites",
	"official_sites": [
		{
			"label": "LBL_Galactapedia",
			"attribute": "SMW_GalactapediaUrl"
		}
	],
	"%extension_modules": "List of modules that can extend item data",
	"extension_modules": [
		{
			"name": "Module:Item/Food",
			"type": [
				"Drink.Bottle",
				"Drink.Can",
				"Food.Bar",
				"Food.Box",
				"Food.Consumable",
				"Food.Junk",
				"Food.Plant",
				"Food.Sachet",
				"Food.Tin"
			]
		},
		{
			"name": "Module:Item/FPSConsumable",
			"type": [
				"FPS_Consumable.Hacking"
			]
		},
		{
			"name": "Module:Item/VehicleItem",
			"type": [
				"Bomb",
				"Cooler",
				"DockingCollar",
				"EMP",
				"ExternalFuelTank",
				"Gadget",
				"MiningModifier.Gun",
				"Missile.Missile",
				"Missile.Torpedo",
				"MissileLauncher.MissileRack",
				"PowerPlant.Power",
				"QuantumDrive",
				"QuantumInterdictionGenerator",
				"SalvageHead",
				"SalvageModifier",
				"SalvageModifier.SalvageModifier_TractorBeam",
				"Shield",
				"TowingBeam",
				"TractorBeam",
				"Turret.BallTurret",
				"Turret.CanardTurret",
				"Turret.GunTurret",
				"Turret.NoseMounted",
				"WeaponGun.Gun",
				"WeaponGun.Rocket",
				"WeaponMining.Gun"
			]
		},
		{
			"name": "Module:Item/WeaponAttachment",
			"type": [
				"WeaponAttachment.Barrel",
				"WeaponAttachment.BottomAttachment",
				"WeaponAttachment.IronSight",
				"WeaponAttachment.Magazine",
				"WeaponAttachment.Utility"
			]
		}
	]
}